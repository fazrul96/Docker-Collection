{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "version":"1.0.33",
    "keywords": [
        "framework",
        "laravel",
        "lumen"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2.5",
        "basemkhirat/elasticsearch": "^1.4",
        "dusterio/lumen-passport": "^0.3.1",
        "flipbox/lumen-generator": "^8.0",
        "illuminate/redis": "^7.29",
        "laravel/lumen-framework": "^7.0",
        "laravel/passport": "8.4.4",
        "laravelista/lumen-vendor-publish": "^7.0",
        "lcobucci/jwt": "3.3.3",
        "predis/predis": "^1.1",
        "venturecraft/revisionable": "^1.33"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.5",
        "krlove/eloquent-model-generator": "^1.3"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}
